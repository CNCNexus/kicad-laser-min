    "def closest_point(fp_list,fp):\n",
    "    (x,y)=fp\n",
    "    distance = 999999999\n",
    "    (npx,npy) = (0,0)\n",
    "    \n",
    "    for (xx,yy) in fp_list:\n",
    "        dist2 = (xx-x)**2+(yy-y)**2\n",
    "        if dist2<distance:\n",
    "            (npx,npy) = (xx,yy)\n",
    "            distance=dist2\n",
    "    return (npx,npy)"
   ]
